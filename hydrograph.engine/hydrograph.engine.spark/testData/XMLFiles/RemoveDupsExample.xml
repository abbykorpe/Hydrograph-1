<p:graph name="RemoveDupsExample" xmlns:p="hydrograph/engine/jaxb/main"
	xmlns:it="hydrograph/engine/jaxb/inputtypes" xmlns:ot="hydrograph/engine/jaxb/outputtypes"
	xmlns:spt="hydrograph/engine/jaxb/straightpulltypes"
	xmlns:tt="hydrograph/engine/jaxb/transformtypes" xmlns:ct="hydrograph/engine/jaxb/commandtypes"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="hydrograph/engine/jaxb/main ../../../elt-core/src/main/resources/newxmlschema/main/main.xsd  ">

	<inputs id="input1" batch="0" xsi:type="it:textFileDelimited">
		<outSocket id="out0">
			<schema name="klk">
				<field name="f1" type="java.lang.Integer" />
				<field name="f2" type="java.lang.String" />
				<field name="f3" type="java.lang.String" />
			</schema>
		</outSocket>
		<path uri="testData/inputFiles/Duplicate_Data.txt" />
		<delimiter value="," />
		<strict value="false" />
		<charset value="ISO-8859-1" />

	</inputs>

	<straightPulls id="dedup" xsi:type="spt:removeDups">
		<inSocket id="in0" fromComponentId="input1" fromSocketId="out0" />
		<outSocket id="unused0" type="unused">
			<copyOfInsocket inSocketId="in0" />
		</outSocket>
		<outSocket id="out0" type="out">
			<copyOfInsocket inSocketId="in0" />
		</outSocket>
		<keep value="unique" />
		<primaryKeys>
			<field name="f1" />
		</primaryKeys>
		<secondaryKeys>
			<field name="f2" />
		</secondaryKeys>
		
	</straightPulls>

	<outputs id="output1" xsi:type="ot:textFileDelimited">
		<inSocket fromComponentId="dedup" fromSocketId="out0" id="in0">
			<schema name="kj">
				<field name="f1" type="java.lang.Integer" />
				<field name="f2" type="java.lang.String" />
				<field name="f3" type="java.lang.String" />
			</schema>
		</inSocket>
		<path uri="hydrograph.engine.spark/testData/output/RemoveDupsOut" />
		<delimiter value="," />
		<charset value="ISO-8859-1" />
	</outputs>

	 <outputs id="output2" xsi:type="ot:textFileDelimited">
		<inSocket fromComponentId="dedup" fromSocketId="unused0" id="in0">
			<schema name="kj">
				<field name="f1" type="java.lang.Integer" />
				<field name="f2" type="java.lang.String" />
				<field name="f3" type="java.lang.String" />
				
			</schema>
		</inSocket>
		<path uri="hydrograph.engine.spark/testData/output/RemoveDupsUnused" />
		<delimiter value="," />
		<charset value="ISO-8859-1" />
	</outputs> 


</p:graph>