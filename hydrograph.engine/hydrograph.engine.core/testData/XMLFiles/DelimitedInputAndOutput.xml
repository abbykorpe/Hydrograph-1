<p:graph name ="DelimitedInputAndOutput.xml" xmlns:it="http://www.bitwiseglobal.com/graph/inputtypes"
	xmlns:ot="http://www.bitwiseglobal.com/graph/outputtypes" xmlns:spt="http://www.bitwiseglobal.com/graph/straightpulltypes"
	xmlns:op="http://www.bitwiseglobal.com/graph/operationstypes" xmlns:ct="http://www.bitwiseglobal.com/graph/commandtypes"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.bitwiseglobal.com/graph/main"
	xsi:schemaLocation="http://www.bitwiseglobal.com/graph/main ./main/main.xsd ">

	<inputs id="input1" phase="1" xsi:type="it:text-file-delimited">
		<outSocket id="out0">
			<schema name="outSchema">
				<include_external_schema uri="testData/XMLFiles/schema.xml" />
			</schema>
		</outSocket>
		<dependsOn componentId="ll" />
		<path uri="@{PATH}" />
		<delimiter value="," />
		<has_header value="false" />
		<safe value="true" />
		<charset value="ISO-8859-1" />
		<quote value="asd" />
	</inputs>

	<outputs id="output1" xsi:type="ot:text-file-delimited">
		<inSocket fromComponentId="input1" fromSocketId="out0" id="in0">
			<schema name="outSchema">
			<field name="id" type="java.lang.String" />
				<field name="name" type="java.lang.String" />
				<field name="city" type="java.lang.String" />
			</schema>
		</inSocket>
		<path uri="testData\output\DelimitedComponentsOp" />
		<delimiter value="," />
		<has_header value="true" />
		<charset value="ISO-8859-1" />
		<runtime_properties>
			<property name="prop" value="propValue" />
		</runtime_properties>
		<!-- <quote value="asd" /> -->
	</outputs>
</p:graph>
