<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig xsds/ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/gather_palette.png"
		canvasIconPath="/icons/dummy-canvas.png" name="DummyComponent"
		nameInPalette="Dummy" defaultNamePrefix="Dummy"
		description="This is Default component."
		helpFilePath="/com.bitwise.app.help/html/Components/Dummy.html">
		<tns:category>DUMMY</tns:category>
		<tns:port>
			<tns:portSpecification typeOfPort="out" numberOfPorts="1" changePortCountDynamically="false">
				<tns:port sequenceOfPort="0" linkMandatory="true" labelOfPort="out0" nameOfPort="out0" allowMultipleLinks="true"/>
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="unused" numberOfPorts="1" changePortCountDynamically="false">
				<tns:port sequenceOfPort="0" linkMandatory="true" labelOfPort="un0" nameOfPort="unused0" allowMultipleLinks="true"/>
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="in" numberOfPorts="1" changePortCountDynamically="false">
				<tns:port sequenceOfPort="0" linkMandatory="true" labelOfPort="in0" nameOfPort="in0" allowMultipleLinks="true"/>
			</tns:portSpecification>
		</tns:port>
		
		<tns:property dataType="string" name="name"
			renderer="COMPONENT_NAME_WIDGET" type="USER" group="GENERAL"
			subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" />

		<tns:property dataType="string" name="xml_properties_content"
			renderer="XML_CONTENT_WIDGET" type="USER" group="GENERAL" subGroup="CONFIGURATION"
			showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" />
		
		<tns:property dataType="string" name="phase" value="0"
			renderer="PHASE_WIDGET" type="USER" group="GENERAL" subGroup="CONFIGURATION"
			showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" />

	</tns:component>
</tns:Config>