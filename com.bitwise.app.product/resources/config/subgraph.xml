<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig xsds/ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/subgraph_palette.png"
		canvasIconPath="/icons/subgraph-canvas.png" name="SubgraphComponent"
		nameInPalette="Subgraph" defaultNamePrefix="Subgraph"
		description="This is Default component.\nsome more text">
		<tns:category>SUBGRAPH</tns:category>
		<tns:port>
			<tns:portSpecification>
				<tns:typeOfPort>out</tns:typeOfPort>
				<tns:numberOfPorts>1</tns:numberOfPorts>
				<tns:sequenceOfPort>1</tns:sequenceOfPort>
				<tns:allowMultipleLinks>true</tns:allowMultipleLinks>
				<tns:linkMandatory>true</tns:linkMandatory>
				<tns:nameOfPort>out1</tns:nameOfPort>
				<tns:labelOfPort>out1</tns:labelOfPort>
			</tns:portSpecification>
			<tns:portSpecification>
				<tns:typeOfPort>unused</tns:typeOfPort>
				<tns:numberOfPorts>1</tns:numberOfPorts>
				<tns:sequenceOfPort>0</tns:sequenceOfPort>
				<tns:allowMultipleLinks>true</tns:allowMultipleLinks>
				<tns:linkMandatory>false</tns:linkMandatory>
				<tns:nameOfPort>unused0</tns:nameOfPort>
				<tns:labelOfPort>un0</tns:labelOfPort>
			</tns:portSpecification>
			<tns:portSpecification>
				<tns:typeOfPort>in</tns:typeOfPort>
				<tns:numberOfPorts>1</tns:numberOfPorts>
				<tns:sequenceOfPort>1</tns:sequenceOfPort>
				<tns:allowMultipleLinks>true</tns:allowMultipleLinks>
				<tns:linkMandatory>true</tns:linkMandatory>
				<tns:nameOfPort>in1</tns:nameOfPort>
				<tns:labelOfPort>in1</tns:labelOfPort>
			</tns:portSpecification>
		</tns:port>
		
		<tns:changeInPortsDynamically/>
		<tns:changeOutPortsDynamically/>
		<tns:changeUnusedPortsDynamically/>
		
		<tns:property dataType="string" name="name"
			renderer="COMPONENT_NAME_WIDGET" type="USER" group="GENERAL"
			subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" />

		<tns:property dataType="string" name="phase" value="0"
			renderer="PHASE_WIDGET" type="USER" group="GENERAL" subGroup="CONFIGURATION"
			showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" />

		<tns:property dataType="string" name="xml_properties_content"
			renderer="XML_CONTENT_WIDGET" type="USER" group="GENERAL" subGroup="CONFIGURATION"
			showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" />

	</tns:component>
</tns:Config>