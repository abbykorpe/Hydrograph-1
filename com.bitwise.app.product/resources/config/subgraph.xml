<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig xsds/ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/subgraph_palette.png"
		canvasIconPath="/icons/subgraph-canvas.png" name="SubgraphComponent"
		nameInPalette="Subgraph" defaultNamePrefix="Subgraph"
		description="This is Subgraph component."
		helpFilePath="/com.bitwise.app.help/html/Components/Dummy.html">
		<tns:category>SUBGRAPH</tns:category>
			<tns:port>
			<tns:portSpecification typeOfPort="in" numberOfPorts="0" changePortCountDynamically="true">
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="out" numberOfPorts="0" changePortCountDynamically="true">
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="unused" numberOfPorts="0" changePortCountDynamically="false">
			</tns:portSpecification>
		</tns:port>
		
			<tns:property dataType="string" name="name"
			renderer="COMPONENT_NAME_WIDGET" type="USER" group="GENERAL"
			subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" propertyHelpText="Displays Name provided by the user"/>

		<tns:property dataType="string" name="path" renderer="FILE_PATH_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="LINK"
			usage="REQUIRED" propertyHelpText="Displays the location of the Subjob file \neither Remote/Local. Allows only underscore ‘_’ and \nalphanumeric characters.">
			<tns:validator>FILE_EXISTS_VALIDATOR</tns:validator>
		</tns:property>
		
		<tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="runtime_properties"
			renderer="SUBGRAPH_PROPERTIES_WIDGET" type="USER" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="OPTIONAL" propertyHelpText="Resolves parameters within components \nin a subgraph."/>

	</tns:component>
</tns:Config>