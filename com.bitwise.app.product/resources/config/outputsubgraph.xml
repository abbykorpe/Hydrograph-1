<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig xsds/ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/output_subgraph_palette.png"
		canvasIconPath="/icons/output_subgraph_canvas.png" name="OutputSubgraphComponent"
		nameInPalette="OutputSubgraph" defaultNamePrefix="Subgraph"
		description="Represents the out sockets for a subgraph."
		helpFilePath="/com.bitwise.app.help/html/Components/Dummy.html" >
		<tns:category>SUBGRAPH</tns:category>
		
			<tns:port>
			<tns:portSpecification typeOfPort="in" numberOfPorts="1" changePortCountDynamically="true">
				<tns:port sequenceOfPort="0" allowMultipleLinks="false" linkMandatory="true" nameOfPort="in0" labelOfPort="in0"/>
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="out" numberOfPorts="0" changePortCountDynamically="false">
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="unused" numberOfPorts="0" changePortCountDynamically="false">
			</tns:portSpecification>
		</tns:port>			
		
		<tns:property dataType="string" name="name" renderer="COMPONENT_NAME_WIDGET" type="USER"
			group="GENERAL" subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED" propertyHelpText="Displays Name provided by the user"/>
		
		<tns:property dataType="string" name="inPortCount" value="1" renderer="INPUT_COUNT_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED" propertyHelpText="Specifies number of input ports. Default 1">
			<tns:validator>INTEGER_VALIDATOR</tns:validator>
		</tns:property>
		
		</tns:component>
</tns:Config>