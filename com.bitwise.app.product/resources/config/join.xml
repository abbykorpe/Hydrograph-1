<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/joinimg.png" canvasIconPath="/icons/join_canvas.png"
		name="Join" nameInPalette="Join" defaultNamePrefix="Join">
		<tns:category>TRANSFORM</tns:category>
		<tns:port>
			<tns:portSpecification>
				<tns:typeOfPort>in</tns:typeOfPort>
				<tns:numberOfPorts>2</tns:numberOfPorts>
				<tns:sequenceOfPort>1</tns:sequenceOfPort>
				<tns:allowMultipleLinks>false</tns:allowMultipleLinks>
				<tns:linkMandatory>true</tns:linkMandatory>
				<tns:nameOfPort>unused0</tns:nameOfPort>
				
			</tns:portSpecification>
			<tns:portSpecification>
				<tns:typeOfPort>in</tns:typeOfPort>
				<tns:numberOfPorts>2</tns:numberOfPorts>
				<tns:sequenceOfPort>2</tns:sequenceOfPort>
				<tns:allowMultipleLinks>false</tns:allowMultipleLinks>
				<tns:linkMandatory>true</tns:linkMandatory>
				<tns:nameOfPort>unused1</tns:nameOfPort>
			</tns:portSpecification>
			<tns:portSpecification>
				<tns:typeOfPort>out</tns:typeOfPort>
				<tns:numberOfPorts>1</tns:numberOfPorts>
				<tns:sequenceOfPort>1</tns:sequenceOfPort>
				<tns:allowMultipleLinks>false</tns:allowMultipleLinks>
				<tns:linkMandatory>false</tns:linkMandatory>
				<tns:nameOfPort>unused0</tns:nameOfPort>
			</tns:portSpecification>
		</tns:port>
		
		<tns:property dataType="string" name="name" renderer="COMPONENT_NAME_WIDGET" type="USER"
			group="GENERAL" subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED"/>
			
		<tns:property dataType="string" name="input_count" renderer="INPUT_COUNT_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED"/>
			
		<tns:property dataType="string" name="join_type" renderer="JOIN_TYPE_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="REQUIRED"/>
			
		<tns:property dataType="string" name="phase" value="0" renderer="PHASE_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED"/>
		
		<tns:property dataType="string" name="runtime_properties" renderer="RUNTIME_PROPERTIES_WIDGET"
			type="USER" group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="OPTIONAL"/>
		
	</tns:component>
</tns:Config>
