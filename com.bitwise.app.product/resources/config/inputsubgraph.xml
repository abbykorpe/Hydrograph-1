<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig xsds/ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/input_subgraph_palette.png"
		canvasIconPath="/icons/input_subgraph_canvas.png" name="InputSubgraphComponent"
		nameInPalette="InputSubgraph" defaultNamePrefix="Subgraph"
		description="This is Input Subgraph Component.\nIt represents in-sockets of a subgraph"
		helpFilePath="/com.bitwise.app.help/html/Components/InputSubgraph.html" >
		<tns:category>SUBGRAPH</tns:category>
		
			<tns:port>
			<tns:portSpecification typeOfPort="in" numberOfPorts="0" changePortCountDynamically="false" >
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="out" numberOfPorts="1" changePortCountDynamically="true">
				<tns:port sequenceOfPort="0" allowMultipleLinks="false" linkMandatory="true" nameOfPort="out0" labelOfPort="out0"/>
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="unused" numberOfPorts="0" changePortCountDynamically="false">
			</tns:portSpecification>
		</tns:port>			
		
		<tns:property dataType="string" name="name" renderer="COMPONENT_NAME_WIDGET" type="USER" group="GENERAL"
			subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" propertyHelpText="Displays Name provided by the user"/>
			
		<tns:property dataType="string" name="outPortCount" value="1" renderer="OUTPUT_COUNT_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED" propertyHelpText="Specifies number of output ports. Default 1">
			<tns:validator>INTEGER_VALIDATOR</tns:validator>
		</tns:property>
			
	</tns:component>
</tns:Config>