<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig xsds/ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/subgraph_palette.png"
		canvasIconPath="/icons/subgraph-canvas.png" name="InputSubgraphComponent"
		nameInPalette="InputSubgraph" defaultNamePrefix="Subgraph"
		description="This is Default component.\nsome more text"
		helpFilePath="/com.bitwise.app.help/html/Components/Dummy.html" >
		<tns:category>SUBGRAPH</tns:category>
			<tns:port>
			<tns:portSpecification typeOfPort="in" numberOfPorts="0" changePortCountDynamically="true">
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="out" numberOfPorts="1" changePortCountDynamically="true">
				<tns:port sequenceOfPort="0" allowMultipleLinks="false" linkMandatory="true" nameOfPort="out0" labelOfPort="out0"/>
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="unused" numberOfPorts="0" changePortCountDynamically="true">
			</tns:portSpecification>
		</tns:port>
	
			
			<tns:property dataType="string" name="name"
			renderer="COMPONENT_NAME_WIDGET" type="USER" group="GENERAL"
			subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT" usage="REQUIRED" />
	
				<tns:property dataType="string" name="outPortCount" value="1" renderer="OUTPUT_COUNT_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED">
			<tns:validator>INTEGER_VALIDATOR</tns:validator>
		</tns:property>
			
	</tns:component>
</tns:Config>