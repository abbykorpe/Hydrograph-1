<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig xsds/ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/lookup_palette.png" canvasIconPath="/icons/lookup_canvas.png"
		name="Lookup" nameInPalette="Lookup" defaultNamePrefix="Lookup" 
		description="This is Lookup component.\nsome more text"
		helpFilePath="/com.bitwise.app.help/html/Components/Lookup.html">
		<tns:category>TRANSFORM</tns:category>
		<tns:port>
			<tns:portSpecification typeOfPort="in" numberOfPorts="2" changePortCountDynamically="false">
				<tns:port sequenceOfPort="0" linkMandatory="true" labelOfPort="drv" nameOfPort="in0" allowMultipleLinks="false"/>
				<tns:port sequenceOfPort="1" linkMandatory="true" labelOfPort="lkp" nameOfPort="in1" allowMultipleLinks="false"/>
			</tns:portSpecification>
			<tns:portSpecification typeOfPort="out" numberOfPorts="1" changePortCountDynamically="false">
				<tns:port sequenceOfPort="0" linkMandatory="true" labelOfPort="out0" nameOfPort="out0" allowMultipleLinks="false"/>
			</tns:portSpecification>
		</tns:port>
		
		<tns:property dataType="string" name="name" renderer="COMPONENT_NAME_WIDGET" type="USER"
			group="GENERAL" subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED"/>
			
		<tns:property dataType="string" name="schema" renderer="JOIN_FIXED_WIDTH_SCHEMA_WIDGET" type="USER"
			group="SCHEMA" subGroup="RECORD_STRUCTURE_DETAILS" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="REQUIRED">
			<tns:validator>SCHEMA_GRID_VALIDATOR</tns:validator>
		</tns:property>
			
		<tns:property dataType="string" name="runtime_properties" renderer="RUNTIME_PROPERTIES_WIDGET"
			type="USER" group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="OPTIONAL"/>
		
		<tns:property dataType="string" name="phase" value="0" renderer="PHASE_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED">
			<tns:validator>INTEGER_VALIDATOR</tns:validator>
		</tns:property>
			
		<tns:property dataType="string" name="hash_join"   renderer="HASH_JOIN_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED">
			<tns:validator>LOOKUP_CONFIG_VALIDATOR</tns:validator>
		</tns:property>
		
		<tns:property dataType="string" name="hash_join_map"   renderer="HASH_JOIN_MAPPING_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED">
			<tns:validator>LOOKUP_MAPPING_VALIDATOR</tns:validator>
		</tns:property>
			
		<tns:property dataType="string" name="match_value"   renderer="MATCH_PROPERTY_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED"/>
		</tns:component>
</tns:Config>
