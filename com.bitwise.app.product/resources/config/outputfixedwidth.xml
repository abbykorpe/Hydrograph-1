<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bitwise.com/ComponentConfig xsds/ComponentConfig.xsd ">
	<tns:component paletteIconPath="/icons/output_fixwidth_palette.png" canvasIconPath="/icons/output_fixwidth_canvas.png"
		name="OFixedWidth" nameInPalette="File Fixed Width" defaultNamePrefix="OFixedWidth" 
		description="This is OFixedWidth component.\nsome more text"
		helpFilePath="/com.bitwise.app.help/html/Components/Output_File_Fixed_Width.html">
		<tns:category>OUTPUT</tns:category>
		<tns:port>
			<tns:portSpecification typeOfPort="in" numberOfPorts="1" changePortCountDynamically="false">
				<tns:port sequenceOfPort="0" linkMandatory="true" labelOfPort="in0" nameOfPort="in0" allowMultipleLinks="false"/>
			</tns:portSpecification>
		</tns:port>
		
		<tns:property dataType="string" name="name" renderer="COMPONENT_NAME_WIDGET" type="USER"
			group="GENERAL" subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED" propertyHelpText="Displays Name provided by the user"/>
		
		<tns:property dataType="string" name="schema" renderer="FIXED_WIDGET" type="USER"
			group="SCHEMA" subGroup="RECORD_STRUCTURE_DETAILS" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="REQUIRED">
			<tns:validator>SCHEMA_GRID_VALIDATOR</tns:validator>
		</tns:property>
		
		<tns:property dataType="string" name="path" renderer="FILE_PATH_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="LINK"
			usage="REQUIRED" propertyHelpText="Displays the location of the file either Remote/Local. \nAllows only underscore ‘_’ and alphanumeric characters.">
			<tns:validator>NON_EMPTY_STRING_VALIDATOR</tns:validator>
		</tns:property>
		
		<tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="strict" value="False"
			renderer="STRICT_CLASS_WIDGET" type="USER" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="OPTIONAL" propertyHelpText="When True, validates the Record Structure \nlike no. of fields, length of record, \ncale as specified by the user.">
			<tns:validator>BOOLEAN_OR_PARAMETER_VALIDATOR</tns:validator>
		</tns:property>
		
		<tns:property dataType="boolean" name="safe" renderer="SAFE_PROPERTY_WIDGET" type="USER" value="False"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="OPTIONAL" propertyHelpText="When True, allows the input fields \nto pass evaluation despite of Data \ntype mismatch.">
			<tns:validator>BOOLEAN_OR_PARAMETER_VALIDATOR</tns:validator>
		</tns:property>
		
		<tns:property dataType="string" name="charset" value="UTF-8" renderer="CHARACTER_SET_WIDGET"
			type="USER" group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="REQUIRED" propertyHelpText="Denotes the encoding system \nfollowed by the input file.">
			<tns:validator>NON_EMPTY_STRING_VALIDATOR</tns:validator>
		</tns:property>
		
		<tns:property dataType="string" name="runtime_properties" renderer="RUNTIME_PROPERTIES_WIDGET"
			type="USER" group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="FALSE" tooltipDataType="NONE"
			usage="OPTIONAL" propertyHelpText="Used to override runtime \nHadoop configurations specific to this \ncomponent at run time."/>
		
		<tns:property dataType="string" name="phase" value="0" renderer="PHASE_WIDGET" type="USER"
			group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"
			usage="REQUIRED" propertyHelpText="Specifies the execution sequence of \nthis component in the job flow.">
			<tns:validator>INTEGER_VALIDATOR</tns:validator>
		</tns:property>
	</tns:component>
</tns:Config>
