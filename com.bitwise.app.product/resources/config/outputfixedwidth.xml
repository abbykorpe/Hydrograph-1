<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bitwise.com/ComponentConfig ComponentConfig.xsd ">
  <tns:component iconPath="/icons/output.png" name="OutputFixedWidth" shape="OutputFixedWidthFigure">
    <tns:category>OUTPUT</tns:category>
 <tns:port>
		<tns:portSpecification>
		    <tns:typeOfPort>in</tns:typeOfPort>
		    <tns:numberOfPorts>1</tns:numberOfPorts>
			<tns:sequenceOfPort>1</tns:sequenceOfPort>
			<tns:allowMultipleLinks>false</tns:allowMultipleLinks>
			<tns:linkMandatory>true</tns:linkMandatory>
		</tns:portSpecification>
		
	</tns:port>
    <tns:property dataType="string" name="name" renderer="ELTComponentNameWidget" type="USER" group="GENERAL" subGroup="DISPLAY"/>
    <tns:property dataType="string" group="FIELD_SEQUENCE" subGroup="CONFIGURATION" name="schema" renderer="ELTFieldSequenceWidget" type="USER" />
    <tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="path" renderer="ELTFilePathWidget" type="USER"/>
  	<tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="strict" renderer="ELTStrictClassWidget" type="USER"/>
    <tns:property dataType="boolean" group="GENERAL" subGroup="CONFIGURATION" name="has_header" renderer="ELTHasHeaderWidget" type="USER"/>
    <tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="charset" renderer="ELTCharacterSetWidget" type="USER"/>
    <tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="runtime_properties" renderer="ELTRuntimePropertiesWidget" type="USER"/>
    <tns:property dataType="string" name="phase" renderer="ELTPhaseWidget" type="USER" group="GENERAL" subGroup="CONFIGURATION" />
  </tns:component>
</tns:Config>
