<?xml version="1.0" encoding="UTF-8"?>
<tns:Config xmlns:tns="http://www.bitwise.com/ComponentConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bitwise.com/ComponentConfig ComponentConfig.xsd ">
  <tns:component paletteIconPath="/icons/output_palette.png" canvasIconPath="/icons/output_canvas.png" name="OFDelimited" shape="OutputFigure">
    <tns:category>OUTPUT</tns:category>
 <tns:port>
		<tns:portSpecification>
		    <tns:typeOfPort>in</tns:typeOfPort>
		    <tns:numberOfPorts>1</tns:numberOfPorts>
			<tns:sequenceOfPort>1</tns:sequenceOfPort>
			<tns:allowMultipleLinks>false</tns:allowMultipleLinks>
			<tns:linkMandatory>true</tns:linkMandatory>
		</tns:portSpecification>
		
	</tns:port>
    <tns:property dataType="string" name="name" renderer="ELTComponentNameWidget" 
    	type="USER" group="GENERAL" subGroup="DISPLAY" showAsTooltip="TRUE" tooltipDataType="TEXT"/>
    	
    <tns:property dataType="string" group="FIELD_SEQUENCE" subGroup="RECORD_STRUCTURE_DETAILS"
    	 name="schema" renderer="ELTFieldSequenceWidget" type="USER" showAsTooltip="FALSE" tooltipDataType="NONE"/>
    	 
    <tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="path" 
    	renderer="ELTFilePathWidget" type="USER" showAsTooltip="TRUE" tooltipDataType="LINK" />
    	
    <tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="delimiter" value="|" 
    	renderer="ELTDelimeterWidget" type="USER" showAsTooltip="TRUE" tooltipDataType="TEXT"/>
    	
    <tns:property dataType="boolean" group="GENERAL" subGroup="CONFIGURATION" name="has_header" 
    	renderer="ELTHasHeaderWidget" type="USER" showAsTooltip="FALSE" tooltipDataType="NONE"/>
    	
    <tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="charset" value="UTF-8" 
    	renderer="ELTCharacterSetWidget" type="USER" showAsTooltip="FALSE" tooltipDataType="NONE"/>
    	
    <tns:property dataType="string" group="GENERAL" subGroup="CONFIGURATION" name="runtime_properties" 
    	renderer="ELTRuntimePropertiesWidget" type="USER" showAsTooltip="FALSE" tooltipDataType="NONE"/>
    	
    <tns:property dataType="string" name="phase" value="0" renderer="ELTPhaseWidget" type="USER" 
    	group="GENERAL" subGroup="CONFIGURATION" showAsTooltip="TRUE" tooltipDataType="TEXT"/>
    	
  </tns:component>
</tns:Config>
